<?xml version="1.0" encoding="utf8"?>
<opml version="1.1">
  <head>
    <title>{{ site.name }}</title>
  </head>
  <body>
    {% for feed in feeds %}
      <outline type="rss" text="{{ feed.title }}" description="{{ feed.title }}" htmlUrl="{{ feed.blog.url }}" xmlUrl="{{ feed.url }}"/>
    {% endfor %}
  </body>
</opml>
